{
  "name": "test-webpack-koa",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "run-p start:front start:back",
    "start:front": "cross-env NODE_ENV=local node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./build/webpack/webpack.config.development.js --progress --profile --colors --host 0.0.0.0",
    "start:back": "cross-env NODE_ENV=local HOT=1 DEBUG='template:*' node index.js",
    "start": "pm2 kill && pm2 node index.js",
    "dll": "webpack --config ./build/webpack/webpack.config.dll.js --progress --profile --colors",
    "build": "cross-env NODE_ENV=production npm run dll && npm run pack",
    "pack": "webpack --config ./build/webpack/webpack.config.production.js --progress --profile --colors",
    "w": "webpack --config ./build/webpack/webpack.config.development.js --progress --profile --colors",
    "analyzer": "cross-env ANALYZER=1 webpack --config ./build/webpack/webpack.config.production.js --progress --profile --colors"
  },
  "repository": {
    "type": "git",
